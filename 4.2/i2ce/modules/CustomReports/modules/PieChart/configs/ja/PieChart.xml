<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="CustomReports_PieChart" path="/modules/CustomReports/displays/PieChart">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="CustomReports_PieChart" path="/modules/CustomReports/displays/PieChart" locale="ja">
    <version>4.2.0.1</version>
    <configuration name="error_message" locale="ja" values="single" type="string">
      <value>レポートにデータがありません。レポートデータのキャッシュを確認するか検索条件を変更して下さい。</value>
    </configuration>
    <configurationGroup name="display_options">
      <configurationGroup name="styles">
        <configurationGroup name="Pie">
          <configuration name="display_name" locale="ja" values="single" type="string">
            <value>円グラフ (簡易)</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="Bar">
          <configuration name="display_name" locale="ja" values="single" type="string">
            <value>棒グラフ (簡易)</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="Column">
          <configuration name="display_name" locale="ja" values="single" type="string">
            <value>縦棒グラフ（簡易）</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="StackedColumn">
          <configuration name="display_name" locale="ja" values="single" type="string">
            <value>積み上げ縦棒グラフ(簡易)</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="Scatter">
          <configuration name="display_name" locale="ja" values="single" type="string">
            <value>散布図</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>