      </configuration>
      <configuration name="custom_reports_delete" locale="zh_CN" values="single" type="string">
        <value>允许删除关于自定义报告的数据</value>
      </configuration>
      <configuration name="custom_reports_can_access_relationships" locale="zh_CN" values="single" type="string">
        <value>允许顾客报告关系的访问</value>
      </configuration>
      <configuration name="custom_reports_can_edit_relationships" locale="zh_CN" values="single" type="string"></configuration>
      <configuration name="custom_reports_can_access_reports" locale="zh_CN" values="single" type="string">
        <value>允许访问自定义报告</value>
      </configuration>
      <configuration name="custom_reports_can_edit_reports" locale="zh_CN" values="single" type="string">
        <value>允许编辑自定义报告</value>
      </configuration>
      <configuration name="custom_reports_can_view_reportViews" locale="zh_CN" values="single" type="string">
        <value>允许浏览自定义报告视图</value>
      </configuration>
      <configuration name="custom_reports_can_edit_reportViews" locale="zh_CN" values="single" type="string">
        <value>允许编辑自定义报告视图</value>
      </configuration>
      <configuration name="custom_reports_admin" locale="zh_CN" values="single" type="string"></configuration>
