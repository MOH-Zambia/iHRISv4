        <configurationGroup name="args">
          <configuration name="title" locale="zh_CN" values="single" type="string">
            <value>查看报告</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="nodisplay">
        <configurationGroup name="args">
          <configuration name="title" locale="zh_CN" values="single" type="string">
            <value>没有有效的显示</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
