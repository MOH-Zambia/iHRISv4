    <configurationGroup name="command_line">
      <configurationGroup name="show">
        <configurationGroup name="args">
          <configuration name="title" locale="zh_CN" values="single" type="string">
            <value>查看报告</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
