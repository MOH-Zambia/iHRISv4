<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="Lists" path="/I2CE/">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="Lists" path="/I2CE/" locale="fr_ML">
    <version>4.2.0.9.1</version>
    <configurationGroup name="auto_list" path="/I2CE/page/auto_list">
      <configuration name="title" locale="fr_ML" values="single" type="string"></configuration>
    </configurationGroup>
    <configurationGroup name="Lists" path="/modules/Lists">
      <configuration name="messages" type="delimited" values="many" locale="fr_ML">
        <value>add_text:Ajouter nouveau %s</value>
        <value>add_new:Ajouter nouveau</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="invalid_messages" path="/modules/forms/invalid_field_messages" locale="fr_ML">
      <configuration name="unique_fields" values="single" type="string"></configuration>
      <configuration name="unique_field" values="single" type="string"></configuration>
      <configuration name="unique" values="single" type="string"></configuration>
    </configurationGroup>
    <configurationGroup name="template_text" path="/modules/forms/template_text">
      <configuration name="blank" values="single" locale="fr_ML" type="string"></configuration>
      <configuration name="loading" values="single" locale="fr_ML" type="string"></configuration>
      <configuration name="no_results" values="single" locale="fr_ML" type="string"></configuration>
      <configuration name="default_display" values="single" locale="fr_ML" type="string"></configuration>
      <configuration name="clear_display" values="single" locale="fr_ML" type="string"></configuration>
    </configurationGroup>
    <configurationGroup name="formClasses" path="/modules/forms/formClasses">
      <configurationGroup name="I2CE_List">
        <configurationGroup name="fields">
          <configurationGroup name="remap">
            <configuration name="headers" type="delimited" locale="fr_ML" values="single"></configuration>
          </configurationGroup>
          <configurationGroup name="i2ce_hidden">
            <configuration name="headers" type="delimited" locale="fr_ML" values="single">
              <value>default:Masquer</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="I2CE_SimpleList">
        <configurationGroup name="fields">
          <configurationGroup name="name">
            <configuration name="headers" type="delimited" locale="fr_ML" values="single">
              <value>default:Nom</value>
            </configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="I2CE_SimpleCodedList">
        <configurationGroup name="fields">
          <configurationGroup name="code">
            <configuration name="headers" type="delimited" locale="fr_ML" values="single"></configuration>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="fr_ML">
      <configuration name="can_hide_list_members" values="single" type="string">
        <value>Peut masquer la liste des membres</value>
      </configuration>
      <configuration name="can_edit_database_lists" values="single" type="string">
        <value>Peut éditer certaines des listes stockées dans la base de données</value>
      </configuration>
      <configuration name="can_edit_all_database_lists" values="single" type="string">
        <value>Peut éditer toutes les listes stockées dans la base de données</value>
      </configuration>
      <configuration name="can_view_database_lists" values="single" type="string">
        <value>Peut voir certaines des listes stockées dans la base de données</value>
      </configuration>
      <configuration name="can_view_all_database_lists" values="single" type="string">
        <value>Peut voir toutes les listes stockées dans la base de données</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="pages" path="/I2CE/page">
      <configurationGroup name="view_list">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="fr_ML" type="string">
            <value>Voir la liste</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>