<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="web-services-lists" path="/modules/web-services">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="web-services-lists" path="/modules/web-services" locale="en_GB">
    <version>4.2.0.0.4.1</version>
    <configurationGroup name="messages" locale="en_GB">
      <configuration name="invalid_list" values="single" type="string">
        <value>'%s' is not a valid list.</value>
      </configuration>
      <configuration name="invalid_form" values="single" type="string">
        <value>'%s' is not a valid form.</value>
      </configuration>
      <configuration name="invalid_field" values="single" type="string">
        <value>'%s' is not a valid field in form '%s'.</value>
      </configuration>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>