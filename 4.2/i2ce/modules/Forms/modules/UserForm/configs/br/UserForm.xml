<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="UserForm" path="/">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="UserForm" path="/" locale="br">
    <version>4.2.0.1.1</version>
    <configurationGroup name="invalid_formfield_message" path="/modules/forms/formfield_messages" locale="br">
      <configuration name="username" path="user/username/unique" values="single" type="string">
        <value>Implijet eo an anv implijer dija.</value>
      </configuration>
      <configuration name="pass_match" path="user/password/mismatch" values="single" type="string">
        <value>Disheñvel e oa a ger-remen hag ar ger-tremen kadarnaet.</value>
      </configuration>
      <configuration name="pass_match" path="user/password/required" values="single" type="string">
        <value>Evit an implijerien nevez e rankit ebarzhiñ ur ger-tremen kentañ.</value>
      </configuration>
      <configuration name="role_notallowed" path="user/role/notallowed" values="single" type="string"></configuration>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="br">
      <configuration name="users_can_edit" locale="br" values="single" type="string">
        <value>A c'hall krouiñ un implijer evit ar reizhiad</value>
      </configuration>
      <configuration name="users_can_edit_all" locale="br" values="single" type="string">
        <value>A c'hall kemmañ holl implijerien ar reizhiad.</value>
      </configuration>
      <configuration name="can_create_or_edit_users" locale="br" values="single" type="string">
        <value>A c'hall krouiñ pe kemmañ an implijerien er reizhiad.</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="pages" path="/I2CE/page">
      <configurationGroup name="user">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="br" type="string">
            <value>Genel an implijerien/moned</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="view_user">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="br" type="string">
            <value>Genel an implijerien/moned</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="roleForm" path="/modules/forms/forms/role">
        <configuration name="display" values="single" locale="br" type="string">
          <value>Roll</value>
        </configuration>
      </configurationGroup>
      <configurationGroup name="formClasses" path="/modules/forms/formClasses">
        <configurationGroup name="I2CE_Role">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="br" values="single">
                <value>default:Roll</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="assignable">
              <configuration name="headers" type="delimited" locale="br" values="single"></configuration>
            </configurationGroup>
            <configurationGroup name="homepage">
              <configuration name="headers" type="delimited" locale="br" values="single">
                <value>default:Pajenn degemer gwellañ-deuet</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="trickle_up">
              <configuration name="headers" type="delimited" locale="br" values="single">
                <value>default:↓Trickle Up</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="I2CE_User_Form">
          <configurationGroup name="fields">
            <configurationGroup name="username">
              <configuration name="headers" type="delimited" locale="br" values="single">
                <value>default:Anv implijer</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="password">
              <configuration name="headers" type="delimited" values="many" locale="br">
                <value>default:Ger-tremen (lezel gwerc'h evit mirout an hevelep ger-tremen)</value>
                <value>generate:Genel ur ger-tremen nevez</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="role">
              <configuration name="headers" type="delimited" locale="br" values="single">
                <value>default:Roll</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>