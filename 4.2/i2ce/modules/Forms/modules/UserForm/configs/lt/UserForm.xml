<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="UserForm" path="/">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="UserForm" path="/" locale="lt">
    <version>4.2.0.1.1</version>
    <configurationGroup name="invalid_formfield_message" path="/modules/forms/formfield_messages" locale="lt">
      <configuration name="username" path="user/username/unique" values="single" type="string">
        <value>Šis vartotojo vardas jau naudojamas</value>
      </configuration>
      <configuration name="pass_match" path="user/password/mismatch" values="single" type="string">
        <value>Slaptažodis ir patvirtinimo slaptažodis nesutampa.</value>
      </configuration>
      <configuration name="pass_match" path="user/password/required" values="single" type="string"></configuration>
      <configuration name="role_notallowed" path="user/role/notallowed" values="single" type="string"></configuration>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="lt">
      <configuration name="users_can_edit" locale="lt" values="single" type="string">
        <value>Gali sukurti vartotoją sistemoje</value>
      </configuration>
      <configuration name="users_can_edit_all" locale="lt" values="single" type="string">
        <value>Gali redaguoti visus vartotojus sistemoje.</value>
      </configuration>
      <configuration name="can_create_or_edit_users" locale="lt" values="single" type="string">
        <value>Galite kurti arba redaguoti vartotojus sistemoje.</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="pages" path="/I2CE/page">
      <configurationGroup name="user">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="lt" type="string">
            <value>Valdyti Vartotojus/Prieigą</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="view_user">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="lt" type="string">
            <value>Valdyti Vartotojus/Prieigą</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="roleForm" path="/modules/forms/forms/role">
        <configuration name="display" values="single" locale="lt" type="string">
          <value>Vaidmuo</value>
        </configuration>
      </configurationGroup>
      <configurationGroup name="formClasses" path="/modules/forms/formClasses">
        <configurationGroup name="I2CE_Role">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="lt" values="single">
                <value>default:Vaidmuo</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="assignable">
              <configuration name="headers" type="delimited" locale="lt" values="single">
                <value>default:Galite Priskirti Vartotojui</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="homepage">
              <configuration name="headers" type="delimited" locale="lt" values="single"></configuration>
            </configurationGroup>
            <configurationGroup name="trickle_up">
              <configuration name="headers" type="delimited" locale="lt" values="single"></configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="I2CE_User_Form">
          <configurationGroup name="fields">
            <configurationGroup name="username">
              <configuration name="headers" type="delimited" locale="lt" values="single">
                <value>default:Naudotojo vardas</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="password">
              <configuration name="headers" type="delimited" values="many" locale="lt">
                <value>default:Slaptažodis (palikite tuščią jei norite išlaikyti tą patį slaptažodį)</value>
                <value>generate:Generuoti Naują Slaptažodį</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="role">
              <configuration name="headers" type="delimited" locale="lt" values="single">
                <value>default:Vaidmuo</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>