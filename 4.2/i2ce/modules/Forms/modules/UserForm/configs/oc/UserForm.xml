<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="UserForm" path="/">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="UserForm" path="/" locale="oc">
    <version>4.2.0.1.1</version>
    <configurationGroup name="invalid_formfield_message" path="/modules/forms/formfield_messages" locale="oc">
      <configuration name="username" path="user/username/unique" values="single" type="string"></configuration>
      <configuration name="pass_match" path="user/password/mismatch" values="single" type="string"></configuration>
      <configuration name="pass_match" path="user/password/required" values="single" type="string"></configuration>
      <configuration name="role_notallowed" path="user/role/notallowed" values="single" type="string"></configuration>
    </configurationGroup>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="oc">
      <configuration name="users_can_edit" locale="oc" values="single" type="string"></configuration>
      <configuration name="users_can_edit_all" locale="oc" values="single" type="string"></configuration>
      <configuration name="can_create_or_edit_users" locale="oc" values="single" type="string"></configuration>
    </configurationGroup>
    <configurationGroup name="pages" path="/I2CE/page">
      <configurationGroup name="user">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="oc" type="string"></configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="view_user">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="oc" type="string"></configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="roleForm" path="/modules/forms/forms/role">
        <configuration name="display" values="single" locale="oc" type="string">
          <value>Ròtle</value>
        </configuration>
      </configurationGroup>
      <configurationGroup name="formClasses" path="/modules/forms/formClasses">
        <configurationGroup name="I2CE_Role">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="oc" values="single">
                <value>default:Ròtle</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="assignable">
              <configuration name="headers" type="delimited" locale="oc" values="single"></configuration>
            </configurationGroup>
            <configurationGroup name="homepage">
              <configuration name="headers" type="delimited" locale="oc" values="single"></configuration>
            </configurationGroup>
            <configurationGroup name="trickle_up">
              <configuration name="headers" type="delimited" locale="oc" values="single"></configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="I2CE_User_Form">
          <configurationGroup name="fields">
            <configurationGroup name="username">
              <configuration name="headers" type="delimited" locale="oc" values="single">
                <value>default:Nom d'utilizaire</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="password">
              <configuration name="headers" type="delimited" values="many" locale="oc"></configuration>
            </configurationGroup>
            <configurationGroup name="role">
              <configuration name="headers" type="delimited" locale="oc" values="single">
                <value>default:Ròtle</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>