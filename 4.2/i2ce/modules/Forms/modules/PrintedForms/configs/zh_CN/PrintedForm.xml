<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="PrintedForms">
  <metadata>
    <version>4.2.1.0.1</version>
  </metadata>
  <configurationGroup name="PrintedForms" locale="zh_CN">
    <version>4.2.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="zh_CN">
      <configuration name="printed_forms_can_access" values="single" type="string"></configuration>
      <configuration name="printed_forms_admin" values="single" type="string"></configuration>
      <configuration name="printed_forms_all_generate" values="single" type="string"></configuration>
      <configuration name="printed_forms_archive_admin" values="single" type="string"></configuration>
      <configuration name="printed_forms_create_all_archives" values="single" type="string"></configuration>
      <configuration name="printed_forms_view_all_archives" values="single" type="string"></configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="generated_doc">
          <configuration name="display" values="single" locale="zh_CN" type="string"></configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="I2CE_GeneratedDoc">
          <configurationGroup name="fields">
            <configurationGroup name="document">
              <configuration name="headers" type="delimited" locale="zh_CN" values="single">
                <value>default:文档</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="date">
              <configuration name="headers" type="delimited" locale="zh_CN" values="single">
                <value>default:日期</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="description">
              <configuration name="headers" type="delimited" values="many" locale="zh_CN">
                <value>default:说明</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="primary_form">
              <configuration name="headers" type="delimited" values="many" locale="zh_CN"></configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>