<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ihris-qualify-PersonTraining" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="ihris-qualify-PersonTraining" path="/I2CE" locale="de">
    <version>4.3.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="de">
      <configuration name="person_can_view_child_form_person_training" locale="de"></configuration>
      <configuration name="person_can_edit_child_form_person_training" locale="de"></configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="person_training">
          <configuration name="display" values="single" locale="de">
            <value>Weiterbildung</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_PersonTraining">
          <configurationGroup name="fields">
            <configurationGroup name="start_date">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Anfangsdatum</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="end_date">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Enddatum</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="training">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Schulungskurs</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="provider">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Schulungsanbieter</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="location">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Schulungsort</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="cpd_credit">
              <configuration name="headers" type="delimited" locale="de"></configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="person_training">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="de">
            <value>Weiterbildung</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>