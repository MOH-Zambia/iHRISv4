<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ProviderInstance" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="ProviderInstance" path="/I2CE" locale="pt_BR">
    <version>4.3.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="pt_BR">
      <configuration name="provider_can_view_child_form_provider_instance" locale="pt_BR"></configuration>
      <configuration name="provider_can_edit_child_form_provider_instance" locale="pt_BR"></configuration>
      <configuration name="can_edit_database_list_funder" locale="pt_BR"></configuration>
      <configuration name="can_view_database_list_funder" locale="pt_BR"></configuration>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="provider_instance">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="pt_BR"></configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="fee">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="pt_BR"></configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="editexams">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="pt_BR"></configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="provider_instance">
          <configuration name="display" values="single" locale="pt_BR"></configuration>
        </configurationGroup>
        <configurationGroup name="fee">
          <configuration name="display" values="single" locale="pt_BR"></configuration>
        </configurationGroup>
        <configurationGroup name="funder">
          <configuration name="display" values="single" locale="pt_BR">
            <value>Financiador</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_ProviderInstance">
          <configurationGroup name="list_default" path="meta/list/default">
            <configuration name="display_string" locale="pt_BR"></configuration>
          </configurationGroup>
          <configurationGroup name="fields">
            <configurationGroup name="cancelled">
              <configuration name="headers" type="delimited" locale="pt_BR">
                <value>default:Cancelado?</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="start_date">
              <configuration name="headers" type="delimited" locale="pt_BR">
                <value>default:Data inicial</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="end_date">
              <configuration name="headers" type="delimited" locale="pt_BR">
                <value>default:Data final</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="academic_year">
              <configuration name="headers" type="delimited" locale="pt_BR">
                <value>default:Ano Acadêmico (ex: 1 Ano)</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="calender_year">
              <configuration name="headers" type="delimited" locale="pt_BR">
                <value>default:Ano Calendário</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="participants_list">
              <configuration name="headers" type="delimited" locale="pt_BR"></configuration>
            </configurationGroup>
            <configurationGroup name="funder">
              <configuration name="headers" type="delimited" locale="pt_BR">
                <value>default:Fonte de financiamento</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="job">
              <configuration name="headers" type="delimited" locale="pt_BR">
                <value>default:Grupo(s) Alvo</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="location">
              <configuration name="headers" type="delimited" locale="pt_BR"></configuration>
            </configurationGroup>
            <configurationGroup name="providertraining">
              <configuration name="headers" type="delimited" locale="pt_BR"></configuration>
            </configurationGroup>
            <configurationGroup name="i2ce_hidden">
              <configuration name="headers" type="delimited" locale="pt_BR">
                <value>default:Esconder</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_Fee">
          <configurationGroup name="fields">
            <configurationGroup name="fee_type">
              <configuration name="headers" type="delimited" locale="pt_BR"></configuration>
            </configurationGroup>
            <configurationGroup name="amount">
              <configuration name="headers" type="delimited" locale="pt_BR">
                <value>default:Montante</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>