<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ProviderInstance" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="ProviderInstance" path="/I2CE" locale="de">
    <version>4.3.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="de">
      <configuration name="provider_can_view_child_form_provider_instance" locale="de"></configuration>
      <configuration name="provider_can_edit_child_form_provider_instance" locale="de"></configuration>
      <configuration name="can_edit_database_list_funder" locale="de">
        <value>Kann Liste der Kostenträgerdatenbank bearbeiten.</value>
      </configuration>
      <configuration name="can_view_database_list_funder" locale="de">
        <value>Kann Liste der Kostenträgerdatenbank anzeigen.</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="page">
      <configurationGroup name="provider_instance">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="de"></configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="fee">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="de"></configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="editexams">
        <configurationGroup name="args">
          <configuration name="title" values="single" locale="de">
            <value>Prüfungseinzelheiten setzten</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="provider_instance">
          <configuration name="display" values="single" locale="de">
            <value>Informationen zur geplanten Schulung</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="fee">
          <configuration name="display" values="single" locale="de">
            <value>Gebühreninformationen zur geplanten Schulung</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="funder">
          <configuration name="display" values="single" locale="de">
            <value>Kostenträger</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_ProviderInstance">
          <configurationGroup name="list_default" path="meta/list/default">
            <configuration name="display_string" locale="de"></configuration>
          </configurationGroup>
          <configurationGroup name="fields">
            <configurationGroup name="cancelled">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Abgesagt?</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="start_date">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Anfangsdatum</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="end_date">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Enddatum</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="academic_year">
              <configuration name="headers" type="delimited" locale="de"></configuration>
            </configurationGroup>
            <configurationGroup name="calender_year">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Kalenderjahr</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="participants_list">
              <configuration name="headers" type="delimited" locale="de"></configuration>
            </configurationGroup>
            <configurationGroup name="funder">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Finanzierungsquelle</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="job">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Zielgruppe(n)</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="location">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Standorteinzelheiten</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="providertraining">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Anbieterschulung</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="i2ce_hidden">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Ausblenden</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
        <configurationGroup name="iHRIS_Fee">
          <configurationGroup name="fields">
            <configurationGroup name="fee_type">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Gebühr</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="amount">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Menge</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>