<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="FeeType" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="FeeType" path="/I2CE" locale="de">
    <version>4.3.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="de">
      <configuration name="can_edit_database_list_fee_type" locale="de">
        <value>Die Liste der Gebührenarten bearbeiten</value>
      </configuration>
      <configuration name="can_view_database_list_fee_type" locale="de">
        <value>Die Liste der Gebührenarten anzeigen</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="formsData" path="formsData/forms">
      <configurationGroup name="unit">
        <configurationGroup name="total">
          <configuration name="fields" value="many" type="delimited" locale="de">
            <value>name:Gesamt</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="per_day">
          <configuration name="fields" value="many" type="delimited" locale="de">
            <value>name:Pro Tag</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="student_per_day">
          <configuration name="fields" value="many" type="delimited" locale="de">
            <value>name:Pro Teilnehmertag</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="facilitator_per_day">
          <configuration name="fields" value="many" type="delimited" locale="de">
            <value>name:Pro Schulungsleiter Pro Tag</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="student">
          <configuration name="fields" value="many" type="delimited" locale="de">
            <value>name:Pro Student</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="facilitator">
          <configuration name="fields" value="many" type="delimited" locale="de">
            <value>name:Pro Dozent</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="fee_type">
          <configuration name="display" values="single" locale="de">
            <value>Gebühreninformation</value>
          </configuration>
        </configurationGroup>
        <configurationGroup name="unit">
          <configuration name="display" values="single" locale="de">
            <value>Einheit</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_FeeType">
          <configurationGroup name="fields">
            <configurationGroup name="name">
              <configuration name="headers" type="delimited" locale="de"></configuration>
            </configurationGroup>
            <configurationGroup name="unit">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Einheit</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="training_length_type">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Art der Schulungsdauer</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>