<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE I2CEConfiguration SYSTEM "I2CE_Configuration.dtd">
<I2CEConfiguration name="ProviderTraining" path="/I2CE">
  <metadata>
    <version>4.3.0.1</version>
  </metadata>
  <configurationGroup name="ProviderTraining" path="/I2CE" locale="de">
    <version>4.3.0.1</version>
    <configurationGroup name="tasks" path="/I2CE/tasks/task_description" locale="de">
      <configuration name="can_edit_database_list_providertraining" locale="de">
        <value>Die Liste der Anbieterschulungen bearbeiten</value>
      </configuration>
      <configuration name="can_view_database_list_providertraining" locale="de">
        <value>Die Liste der Anbieterschulungen anzeigen</value>
      </configuration>
    </configurationGroup>
    <configurationGroup name="forms" path="/modules/forms">
      <configurationGroup name="forms">
        <configurationGroup name="providertraining">
          <configuration name="display" values="single" locale="de">
            <value>Informationen zur Anbieterschulung</value>
          </configuration>
        </configurationGroup>
      </configurationGroup>
      <configurationGroup name="formClasses">
        <configurationGroup name="iHRIS_ProviderTraining">
          <configurationGroup name="fields">
            <configurationGroup name="trainingprovider">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Anbieter</value>
              </configuration>
            </configurationGroup>
            <configurationGroup name="training">
              <configuration name="headers" type="delimited" locale="de">
                <value>default:Schulung</value>
              </configuration>
            </configurationGroup>
          </configurationGroup>
        </configurationGroup>
      </configurationGroup>
    </configurationGroup>
  </configurationGroup>
</I2CEConfiguration>